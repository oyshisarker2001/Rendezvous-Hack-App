{
    "name": "@coral-protocol/coral-studio",
    "private": false,
    "version": "0.1.3",
    "type": "module",
    "files": [
        "build"
    ],
    "bin": "build/server.js",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && tsc --outDir build/ -m preserve socketio.ts && shx cp server.js build/.",
        "preview": "node build/server.js",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "prepare": "svelte-kit sync && yarn build"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@internationalized/date": "^3.8.1",
        "@lucide/svelte": "^0.523.0",
        "@sveltejs/adapter-node": "^5.2.12",
        "@sveltejs/kit": "^2.16.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@tailwindcss/vite": "^4.0.0",
        "bits-ui": "^2.8.6",
        "clsx": "^2.1.1",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^3.0.0",
        "globals": "^16.0.0",
        "mode-watcher": "^1.0.8",
        "paneforge": "1.0.0-next.5",
        "phosphor-icons-svelte": "^2.0.9",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "shx": "^0.4.0",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "svelte-sonner": "^1.0.1",
        "tailwind-merge": "^3.0.2",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^4.0.0",
        "tw-animate-css": "^1.3.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.20.0",
        "vite": "^7.1.5"
    },
    "dependencies": {
        "express": "^5.1.0",
        "runed": "^0.28.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1"
    },
    "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
